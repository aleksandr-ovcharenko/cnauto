{
  "build": {
    "env": {
      "FLASK_APP": "backend.app",
      "FLASK_ENV": "production",
      "PORT": "5000"
    }
  },
  "start": {
    "cmd": "flask run --host=0.0.0.0 --port=$PORT",
    "env": {
      "FLASK_APP": "backend.app"
    }
  },
  "deploy": {
    "startCommand": "python backend/app.py"
  },
  "postdeploy": "python backend/seeds/manage.py"
}
